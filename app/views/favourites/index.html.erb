<p id="notice"><%= notice %></p>

<h1>Listing Favourites</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favourites.each do |favourite| %>
      <tr>
        <td><%= favourite.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to search', movie_search_path %>
